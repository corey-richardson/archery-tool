{
  "name": "archery-tool",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate --no-engine && node scripts/generate-swagger.js && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:check": "next lint --max-warnings 0",
    "format:check": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "postinstall": "prisma generate --no-engine",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-data-grid": "^8.9.1",
    "@prisma/client": "^6.12.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "next": "15.4.1",
    "next-auth": "^4.24.11",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.5.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.29.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/next-auth": "^3.15.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/swagger-jsdoc": "^6.0.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "eslint-plugin-jsdoc": "^59.0.1",
    "husky": "^9.1.7",
    "prisma": "^6.12.0",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
